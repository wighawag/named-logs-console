{
  "name": "named-logs-console",
  "version": "0.1.0",
  "description": "console logger using named-logs facade for minimal lib overhead with full control",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "author": "Ronan Sandford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wighawag/named-logs-console.git"
  },
  "keywords": [
    "console",
    "logger",
    "logging"
  ],
  "dependencies": {
    "named-logs": "0.0.13"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "prepare": "node .setup.js && npm run lint && npm run build",
    "dev": "tsc -w -p tsconfig.json",
    "build:esm": "rimraf dist/esm && tsc -p tsconfig.json",
    "build": "run-s build:esm",
    "lint": "eslint src/**/*.{js,ts,tsx} --quiet --fix"
  }
}
